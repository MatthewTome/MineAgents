{
  "name": "mineagents",
  "private": true,
  "version": "0.1.0",
  "description": "Monorepo for MineAgents",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264",
  "scripts": {
    "dev:bot": "pnpm -C apps/bot dev",
    "build:bot": "pnpm -C apps/bot build",
    "test:bot": "pnpm --filter @mineagents/bot test",
    "lint": "",
    "eval:llm": "pnpm -C apps/bot eval:llm",
    "eval:rag": "python py/agent/scripts/evaluate_rag.py"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.8.1",
    "prismarine-block": "^1.22.0",
    "prismarine-entity": "^2.5.0",
    "prismarine-item": "^1.17.0"
  }
}
